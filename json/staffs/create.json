{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "fullName": {
            "type": "string"
        },
        "birthday": {
            "type": "string",
            "format": "date"
        },
        "identificationNumber": {
            "type": "string",
            "pattern": "^([0-9]{9}|[0-9]{12})$"
        },
        "phoneNumber": {
            "type": "string",
            "pattern": "^(0[0-9]{9})$"
        },
        "address": {
            "type": "string"
        },
        "languages": {
            "type": "array",
            "items": {
                "type": "string"
            },
            "uniqueItems": true
        },
        "certificates": {
            "type": "array",
            "items": {
                "type": "string"
            },
            "uniqueItems": true
        },
        "skills": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "techStack": {
                        "type": "string"
                    },
                    "experience": {
                        "type": "string",
                        "enum": ["fresher", "junior", "senior"]
                    }
                }
            },
            "minItems": 1
        },
        "projects": {
            "type": "array",
            "items": {
                "type": "string"
            },
            "uniqueItems": true
        }
        
    },
    "required": ["fullName", "birthday", "identificationNumber", "phoneNumber", "address", "languages", "certificates", "skills"]
}